<%= simple_form_for [:admin, @info_type, @info ],html: {class: 'form-horizontal', multipart: true},  defaults: { }   do |f| %>
  <%= f.input :title, label: "信息标题" %>
  <%=render :partial=>'new_info_details'%>
  <div class="line line-dashed b-b line-lg pull-in"></div>
  <div class="col-sm-12 text-center">
    <%= f.button :submit, value: "保存",  class: "btn btn-primary" %>
    <%= link_to "返回", admin_info_type_infos_path,  class: "btn btn-default" %>
  </div>
<% end %>



