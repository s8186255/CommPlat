<p id="notice"><%= notice %></p>

<h1>Listing User Ceshis</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_ceshis.each do |user_ceshi| %>
      <tr>
        <td><%= user_ceshi.name %></td>
        <td><%= link_to 'Show', user_ceshi %></td>
        <td><%= link_to 'Edit', edit_user_ceshi_path(user_ceshi) %></td>
        <td><%= link_to 'Destroy', user_ceshi, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ceshi', new_user_ceshi_path %>
