<tr>
  <td>
    <%= role.name %>
  </td>
  <td><%= role.desc %></td>
  <td><%= time_f(role.created_at) %></td>
  <td>
    <%= operation_to @role_actions, '显示', admin_role_path(role)   %>
    <%#= operation_to @role_actions, '编辑', edit_admin_role_path(role), class: "btn btn-default btn-sm"  %>
    <%= operation_to @role_actions, '删除', admin_role_path(role), method: :delete, class: "btn btn-default btn-sm", data: { confirm: '确认要删除吗?' } %>
  </td>
</tr>
