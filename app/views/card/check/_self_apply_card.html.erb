<tr>
  <td>
    <%= self_apply_card.expo.name %>
  </td>
  <td>
    <%= self_apply_card.name %>
  </td>
  <td>
    <%= self_apply_card.sn %>
  </td>
  <td>
    <%= self_apply_card.work_unit %>
  </td>
  <td><%= time_f(self_apply_card.created_at) %></td>
  <td>
    <a href="/card/check/self_apply_cards/<%= self_apply_card.id %>?expo_id=<%= params[:expo_id] %>" class="btn btn-sm btn-default">显示</a>
  </td>
  <td>
    <%= link_to '通过', self_apply_card_is_ok_card_check_index_path(id: self_apply_card.id, expo_id: params[:expo_id]), method: :put , class: "btn btn-success btn-sm" %>
    <%= link_to '不通过',"javascript:void(0)", class: "btn btn-danger btn-sm", data: { toggle: "modal", target: "#exampleModal", whatever: self_apply_card.id.to_s  }  %>
  </td>
</tr>
